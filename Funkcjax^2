#include <iostream>
#include <cstdlib>
#include <ctime>

using namespace std;
int f(int x)
{
    return (x*x)-(5*x)+6;

}

int main()
{
    int a;
    srand (time(NULL));
    for( int i = 0; i < 5; i++ )
    {
        a=rand()%31;
        cout<< a<< "\t" <<f(a) <<endl;
    }
return 0;
}



int P, P1, P2, P3, P4;




P1=dl*f*(a+(dl)/2);
P2=dl*f*(a+dl+(dl)/2);
P3=dl*f*(a+2*dl+(dl)/2);
P4=dl*f*(a+3*dl+(dl)/2);

P=P1+P2+P3+P4;
cout<<P;
